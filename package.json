    "ts-jest": "^26.3.0",
    "tslint-to-eslint-config": "^0.3.1",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "npx tsc -p tsconfig.json",
    "build:watch": "npm run build -- -w",
    "test": "npx jest",
    "test:watch": "npm test -- --watch",
    "format": "npx prettier --write \"src/**/*.{js,ts}\"",
    "lint": "npx eslint --ext .js,.ts ./src",
