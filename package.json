{
  "name": "@codycraven/data-structs",
  "version": "0.1.8",
  "description": "Common data structures for JavaScript",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "runkitExampleFilename": "./example.js",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.13",
